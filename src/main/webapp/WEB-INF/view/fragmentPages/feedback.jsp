<div id="feedBackSection">
	<h1 class="text-center">Feedback</h1>
	<div class="container w-25">
		<form>
			<div class="form-group row">
				<label class="control-label" for="input01">Name</label> <input
					type="text" class="form-control" id="input01">
			</div>
			<div class="form-group row">
				<label class="control-label" for="input11">Email</label> <input
					type="text" class="form-control" id="input11">
			</div>
			<div class="form-group row">
				<label class="control-label" for="textarea">Messages</label>
				<textarea class="form-control" id="textarea" rows="3"
					style="height: 65px"></textarea>
			</div>
			<div class="form-group row">
				<button type="submit" class="btn btn-success">Submit
					Feedback</button>
				<button class="btn">Cancel</button>
			</div>
		</form>
	</div>
</div>